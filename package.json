{
  "name": "enzyme-example-mocha",
  "version": "0.1.0",
  "description": "Example project with React + Enzyme + Mocha",
  "main": "build/index.js",
  "scripts": {
    "test": "./node_modules/karma/bin/karma start --single-run --browsers PhantomJS",
    "test:watch": "./node_modules/karma/bin/karma start",
    "build": "webpack",
    "serve": "webpack-dev-server --watch",
    "journey-js": "serve ./public -p 3999 | node ./test/.selenium.js",
    "journey": "selenium-standalone start | serve ./public -p 3999 | node ./test/.wd-sync.js",
    "watch:test": "nodemon -x 'mocha test/.setup.js test/**/*-test.js'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lelandrichardson/enzyme-example-mocha.git"
  },
