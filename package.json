  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lelandrichardson/enzyme-example-mocha/issues"
  },
  "homepage": "https://github.com/lelandrichardson/enzyme-example-mocha",
  "engines": {
    "node": ">= 7.3.0"
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-airbnb": "^1.0.1",
    "babel-register": "^6.4.3",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "chai": "^3.5.0",
    "css-loader": "0.25.0",
    "enzyme": "^2.0.0",
    "file-loader": "0.9.0",
    "ignore-styles": "^5.0.1",
    "jasmine-core": "^2.4.1",
    "jsdom": "^8.0.1",
    "json-loader": "0.5.4",
    "karma": "^0.13.19",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-browserify": "^5.0.0",
    "karma-chrome-launcher": "^0.2.2",
    "karma-jasmine": "^0.3.7",
    "karma-mocha": "^0.2.1",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.4.5",
    "node-sass": "3.10.1",
    "nodemon": "^1.11.0",
    "phantomjs-prebuilt": "^2.1.4",
