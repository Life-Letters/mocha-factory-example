{
  "name": "enzyme-example-mocha",
  "version": "0.1.0",
  "description": "Example project with React + Enzyme + Mocha",
  "main": "build/index.js",
  "scripts": {
    "test": "nodemon -x 'mocha test/.setup.js test/**/*-test.js'",
    "watch": "webpack -d --watch",
    "build": "webpack",
    "serve": "serve ./public",
    "start": "nodemon",
    "journey-js": "serve ./public -p 3999 | node ./test/.selenium.js",
    "journey": "selenium-standalone start | serve ./public -p 3999 | node ./test/.wd-sync.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lelandrichardson/enzyme-example-mocha.git"
  },
  "author": "Leland Richardson <leland.richardson@airbnb.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lelandrichardson/enzyme-example-mocha/issues"
  },
  "homepage": "https://github.com/lelandrichardson/enzyme-example-mocha",
  "engines": {
    "node": ">= 7.3.0"
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-airbnb": "^1.0.1",
    "babel-register": "^6.4.3",
    "chai": "^3.5.0",
    "enzyme": "^2.0.0",
    "jsdom": "^8.0.1",
    "mocha": "^2.4.5",
    "nodemon": "^1.11.0",
    "react-addons-test-utils": "^15.4.0",
    "selenium-html-js-converter": "^0.4.7",
    "selenium-webdriver": "^3.0.1",
    "serve": "^1.4.0",
    "wd-sync": "^1.2.5",
    "webpack": "^1.13.1"
  },
  "dependencies": {
    "react": "^15.4.0",
    "react-dom": "^15.4.0"
  }
}
